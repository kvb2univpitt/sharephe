<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:sharephe="http://www.i2b2.org/xsd/cell/sharephe/1.1/"
           targetNamespace="http://www.i2b2.org/xsd/cell/sharephe/1.1/">

    <xs:element name="authors" type="sharephe:authorsType" />
    <xs:element name="workbook" type="sharephe:workbookType" />
    <xs:element name="files" type="sharephe:filesType" />
    <xs:element name="sharepheWorkbook" type="sharephe:sharepheWorkbookType" />
    <xs:element name="sharepheWorkbooks" type="sharephe:sharepheWorkbooksType" />

    <xs:complexType name="authorsType">
        <xs:sequence>
            <xs:element name="author" type="xs:string" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="workbookType">
        <xs:sequence>
            <xs:element name="phenotype_id" type="xs:string" />
            <xs:element name="authors" type="sharephe:authorsType"/>
            <xs:element name="institution" type="xs:string" />
            <xs:element name="query_XML" type="xs:string" />
            <xs:element name="s3_address" type="xs:string" />
            <xs:element name="title" type="xs:string" />
            <xs:element name="type" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="filesType">
        <xs:sequence>
            <xs:element name="file" type="xs:string" maxOccurs="unbounded" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="sharepheWorkbookType">
        <xs:sequence>
            <xs:element name="workbook" type="sharephe:workbookType"/>
            <xs:element name="files" type="sharephe:filesType"/>
            <xs:element name="file_URL" type="xs:string" />
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="sharepheWorkbooksType">
        <xs:sequence>
            <xs:element name="sharephe_workbook" type="sharephe:sharepheWorkbookType" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>

</xs:schema>